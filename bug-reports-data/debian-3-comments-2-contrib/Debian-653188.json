            "sentences": [
                {
                    "is_in_lex_summary": false, 
                    "text": "On 12/24/2011 11:12 PM, RoEn wrote:\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "> may be, that extlinux (both 4:04 and 4.05) can not cope with /etc/mtab --> /proc/mounts, and can not parse the device node for the MBR...\n", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "extlinux-update only looks at fstab. ", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "if your fstab is wrong, you get wrong results for extlinux.\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "-- \n", 
                    "is_in_email_summary": false
                }, 
                {
