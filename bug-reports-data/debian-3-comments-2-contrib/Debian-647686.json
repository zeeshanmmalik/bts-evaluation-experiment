            "sentences": [
                {
                    "is_in_lex_summary": false, 
                    "text": "Package: libmikmod2\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "Version: 3.1.12-3\n", 
