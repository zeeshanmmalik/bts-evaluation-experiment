                {
                    "is_in_lex_summary": false, 
                    "text": ">The following packages have unmet dependencies:\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "> perl : Depends: perl-modules (>= 5.14.2-5) but 5.12.4-6 is installed\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
