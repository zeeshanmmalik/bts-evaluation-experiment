            "sentences": [
                {
                    "is_in_lex_summary": true, 
                    "text": "openbox 3.4 contains a debian directory which has much of the required\nthings in it but it is only missing these:", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "- debian menu support", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "- the alternatives for window manager (update-alternatives --config\nx-window-manager)", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "The alternative for Openbox 3.4 should probably point to", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "\"openbox-session\" rather than \"openbox\" as this provides autostart\ncapabilities on login.", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "But I'm not really sure where the", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "x-window-manager is used, so maybe that would be wrong.", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "For instance,\n\"openbox-session\" should not be used when switching between window\nmanagers in a current X session.", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "Thanks!", 
                    "is_in_email_summary": true
                }
            ], 
            "name": "\"Dana Jansens\" <danakj@orodu.net>", 
            "email": "\"Dana Jansens\" <danakj@orodu.net>"
        }, 
