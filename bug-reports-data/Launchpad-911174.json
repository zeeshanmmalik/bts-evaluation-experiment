            "sentences": [
                {
                    "is_in_lex_summary": false, 
                    "text": "GTK+ 3 hides all private fields of objects using GSEAL. ", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": " device-manager.cpp fails to build, because it uses private fields of GdkDevice (buildlog attached).\n", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "The trouble is that we are defining our own \"input devices\" by modifying fields of GdkDevice. ", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": " The GDK+ API does not support setting these fields, so we need to find another way of doing this.\n", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "Please could someone who is familiar with device-manager take a look at this?\n", 
                    "is_in_email_summary": true
                }
            ], 
