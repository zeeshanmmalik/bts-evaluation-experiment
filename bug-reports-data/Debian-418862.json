                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": " .\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "   * Fix an out-of-bounds error in the line editor. (Closes: #429673)\n .\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "   * Force aptitude to create the state file the first time it's run\n     even if the user didn't install/remove anything, so it knows\n     which packages are new.", 
