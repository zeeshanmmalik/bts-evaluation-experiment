            "sentences": [
                {
                    "is_in_lex_summary": false, 
                    "text": "One might of course argue that most search boxes are redundant since you could just as \neasily hit the omnibar, type in youtube, hit tab and do your search.", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "But that doesn't go for your browser history.", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "Only when there is a search result \navailable within your *recent* history does the omnibar suggest search of history.", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "When \nthere is not the omnibar isn't to any help.", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "So please at least let us have the history search back on the NTP.", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "At least.", 
                    "is_in_email_summary": false
                }
            ], 
