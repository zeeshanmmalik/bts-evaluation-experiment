            "sentences": [
                {
                    "is_in_lex_summary": true, 
                    "text": "Could you clarify the expected result: \"Cannot move bookmark to the child folder\"\n", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "Do you mean that the bookmark cannot be moved because the parent folder is not opened \nautomatically during the drag?", 
