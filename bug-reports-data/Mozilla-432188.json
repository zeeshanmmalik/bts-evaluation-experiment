                    "is_in_lex_summary": false, 
                    "text": "\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": ">-pref(\"extensions.blocklist.detailsURL\", \"http://%LOCALE%.", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "www.mozilla.com/%LOCALE%/blocklist/\");\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": ">+pref(\"extensions.blocklist.detailsURL\", \"http://www.mozillamessaging.com/%LOCALE%/blocklist/\");\n", 
                    "is_in_email_summary": false
                }, 
                {
