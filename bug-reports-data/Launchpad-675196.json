                {
                    "is_in_lex_summary": true, 
                    "text": "bzr push --verbose lp:ocamlbricks/0.90.x", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "I observe the behavior reported by Vadim Nevorotin.", 
