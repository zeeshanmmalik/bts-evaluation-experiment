                {
                    "is_in_lex_summary": false, 
                    "text": "forcedeth.c: Reverse Engineered nForce ethernet driver.", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "Version 0.g0.", 
                    "is_in_email_summary": false
