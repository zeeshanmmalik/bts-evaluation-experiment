                {
                    "is_in_lex_summary": true, 
                    "text": "The Places query system supports serialization of queries into URIs, and\nvice-versa.", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "We need to write tests that exercise the serialization and de-serialization of\nall public query options.", 
                    "is_in_email_summary": true
                }
            ], 
            "name": "Dietrich Ayala (:dietrich)", 
            "email": "Dietrich Ayala (:dietrich)"
        }, 
        {
