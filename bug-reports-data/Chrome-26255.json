        {
            "date": "2009-10-29 17:38:29", 
            "sentences": [
                {
                    "is_in_lex_summary": true, 
                    "text": "There's a color, BrowserThemeProvider::kColorToolbar that's meant to be drawn behind \nany toolbar images for the case where a transparent or short image is supplied (and it \nprovides the color for the one pixel content-edges).", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": " The toolbar image is not meant to \nbe repeated vertically.\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "It's probable that this color is not getting drawn in the bookmark and find bar.\n", 
                    "is_in_email_summary": false
                }
            ], 
            "name": "/u/glen@chromium.org/", 
            "email": "/u/glen@chromium.org/"
        }, 
