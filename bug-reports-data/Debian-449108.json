                    "text": "+++ compiz-0.5.2/debian/patches/series", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "@@ -3,0 +4 @@", 
                    "is_in_email_summary": false
                }, 
                {
