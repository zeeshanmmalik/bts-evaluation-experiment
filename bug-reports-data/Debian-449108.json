                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "--- compiz-0.5.2.orig/debian/patches/016_CVE-2007-3920.patch\n+++ compiz-0.5.2/debian/patches/016_CVE-2007-3920.patch", 
                    "is_in_email_summary": false
