                    "is_in_lex_summary": false, 
                    "text": "# removal of GStreamer 0.8 vs removal of gst-ffmpeg\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "clone 410352 -1\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "retitle -1 Dropping the gstreamer0.8 stack\n", 
