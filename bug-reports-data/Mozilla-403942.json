                {
                    "is_in_lex_summary": false, 
                    "text": "\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "Bug 406342 may or may not also fix this, at the same time that it fixes that\nwindow hiding, which would make this \"fixed by bug 406342\" not \"duplicate of\",\nbut it certainly isn't a duplicate of bug 395334.\n", 
                    "is_in_email_summary": true
                }
            ], 
            "name": "Phil Ringnalda (:philor)", 
