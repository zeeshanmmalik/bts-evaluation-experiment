            "sentences": [
                {
                    "is_in_lex_summary": true, 
                    "text": "We use a file descriptor for every SharedMemory, and there's no limiting on \nhow many we create at a time, so it's possible to exceed the fd limit and fail.", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "Failure isn't handled particularly gracefully (it will generally cause a renderer \ncrash).", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "We need to make it possible for SharedMemory creation to return a \"try again \nlater\" response, and have clients respect that.", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "What steps will reproduce the problem?", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "1. Launch Chromium with --file-descriptor-limit=<a very low number>", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "What is the expected output?", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "What do you see instead?", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "It should be slow, but still render the page correctly.", 
                    "is_in_email_summary": true
                }
            ], 
            "name": "/u/stuartmorgan@chromium.org/", 
            "email": "/u/stuartmorgan@chromium.org/"
