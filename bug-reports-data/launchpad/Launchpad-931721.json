            "sentences": [
                {
                    "is_in_lex_summary": true, 
                    "text": "When I plug in headphones, the sound is very quiet, even when I crank the volume to the maximum.\n", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "If I go to the sound settings and switch the connector from analog headphones to analog speakers, then the sound is fine.\n", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "If I unplug the headphones, the sound comes out of the speakers again just fine.\n", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "If I plug in the headphones again, the sound is low, and I have to go back and change the sound settings. ", 
                    "is_in_email_summary": true
                }, 
