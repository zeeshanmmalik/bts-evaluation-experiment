                {
                    "is_in_lex_summary": false, 
                    "text": "So...", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "I think we want to stick with gtk-find for the generic search textbox\nwidget, but cannot use it for the Firefox search bar.", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "Other opinions?", 
                    "is_in_email_summary": false
                }
