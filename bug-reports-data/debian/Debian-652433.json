                {
                    "is_in_lex_summary": false, 
                    "text": "Long description:\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": " The v3c-qt package ties QT doxygen-generated documentation into the v3c\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": " documentation chain, so that client packages can \"inherit\" this in  their own\n", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": " doxygen-generated documentation.\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": " .\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": " It also provides automake rules for MOC, UIC and RCC, to ease their  usage in\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": " client packages automake Makefile.am files.\n", 
                    "is_in_email_summary": false
                }
            ], 
