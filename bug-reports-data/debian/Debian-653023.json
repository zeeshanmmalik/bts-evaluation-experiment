                {
                    "is_in_lex_summary": false, 
                    "text": "Currently there is no such service. ", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "Right one is described in \n", 
                    "is_in_email_summary": false
