            "sentences": [
                {
                    "is_in_lex_summary": true, 
                    "text": "I had the same problem with gimp crashing, too.", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "The culprit was\n\"~/.", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "local/share/mime/mime.cache\" I deleted it and all is fine!!!", 
                    "is_in_email_summary": false
                }, 
