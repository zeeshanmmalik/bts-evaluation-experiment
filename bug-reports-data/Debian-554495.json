        {
            "date": "2009-11-06 09:36:35", 
            "sentences": [
                {
                    "is_in_lex_summary": true, 
                    "text": "Hi!", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "The simple solution is to logout and login again.", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "The problem is caused\nby a protocol change in gvfs.", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "See http://bugs.debian.org/548898", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "I leave it up to the maintainers to close this bug.", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "Closing this bug\nmight cause more bug reports with the same problem.", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "(And merging\nprobably doesn't make sense anymore, there are 8 other bug reports\nalready merged).", 
                    "is_in_email_summary": false
                }, 
