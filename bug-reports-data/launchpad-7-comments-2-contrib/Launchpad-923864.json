            "date": "2012-01-31 03:17:18", 
            "sentences": [
                {
                    "is_in_lex_summary": true, 
                    "text": "Following configurations need to be enabled for thermal solution to work on samsung origen boards.\n", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "Arch independent\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "CONFIG_HWMON=y\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "CONFIG_THERMAL=y\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "CONFIG_THERMAL_HWMON=y\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "CONFIG_CPU_FREQ=y\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "CONFIG_CPU_FREQ_DEFAULT_GOV_ONDEMAND=y\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "Arch dependent\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "CONFIG_SENSORS_EXYNOS4_TMU=y\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "CONFIG_CPU_THERMAL=y\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "CONFIG_SAMSUNG_THERMAL_INTERFACE=y\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "CONFIG_ARM_EXYNOS4210_CPUFREQ=y\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "CONFIG_EXYNOS4_DEV_TMU=y\n", 
                    "is_in_email_summary": false
                }
            ], 
