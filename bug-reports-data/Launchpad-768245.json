                {
                    "is_in_lex_summary": false, 
                    "text": "", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "Please install ppa-purge and use it to downgrade all of this PPA's packages before the upgrade.", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "Downgrade the packages to the version of the official Ubuntu archive until you find the culprit (webkit and banshee may also cause problems)", 
