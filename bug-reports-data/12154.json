            "sentences": [
                {
                    "is_in_lex_summary": true, 
                    "text": "Fix checked in, but I can't be certain that it fixed the problem.", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "I discovered that \nthe spinlock initialization code in tcmalloc is racey.", 
                    "is_in_email_summary": false
