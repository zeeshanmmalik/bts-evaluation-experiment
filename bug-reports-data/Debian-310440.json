                {
                    "is_in_lex_summary": true, 
                    "text": "reassign <number> busybox", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "in body, where number is a number of bug.", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "You can see it in Subject line.", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "All bugs filed agains busybox-cvs shall be closed RSN.", 
                    "is_in_email_summary": false
