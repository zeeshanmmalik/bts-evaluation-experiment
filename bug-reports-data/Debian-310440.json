                    "is_in_lex_summary": false, 
                    "text": "Hello,", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "Please note, that package busybox-cvs has been removed from unstable[1].", 
                    "is_in_email_summary": false
