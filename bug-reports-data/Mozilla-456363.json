                {
                    "is_in_lex_summary": false, 
                    "text": "3. Delete new event with CalDAV client on computer #1.\n", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "4. Reload calendar with CalDAV client on computer #2.\n", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "Actual Results:  \n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "The deleted meeting still show up in the calendar of the CalDAV client running\non computer #2.\n", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "Expected Results:  \n", 
