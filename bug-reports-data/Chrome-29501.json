            "sentences": [
                {
                    "is_in_lex_summary": true, 
                    "text": "Evidently this is a well-documented feature.", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": ":-) (I pointed out elsewhere that I only \nlearned about this a week or two ago by reading the omnibox source code....)", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "Putting '?'", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "forces a search (rather than applying heuristics to decide if whatever you typed is an \naddress).", 
                    "is_in_email_summary": false
