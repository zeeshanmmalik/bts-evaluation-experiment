                {
                    "is_in_lex_summary": false, 
                    "text": "", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "I'm confused as to why it needs to do the \"?\"", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "thing.", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "Can't it just focus the url bar and \nclear it?", 
                    "is_in_email_summary": false
                }
            ], 
            "name": "/u/pinkerton@chromium.org/", 
