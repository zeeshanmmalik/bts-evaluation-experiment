            "sentences": [
                {
                    "is_in_lex_summary": false, 
                    "text": "Right, I see.", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "  What about username/password?\n", 
                    "is_in_email_summary": false
                }
            ], 
            "name": "Boris Zbarsky (:bz)", 
            "email": "Boris Zbarsky (:bz)"
        }, 
        {
            "date": "2007-09-26 10:02:33", 
            "sentences": [
                {
                    "is_in_lex_summary": true, 
                    "text": "This only operates on the output of the URI classifier's canonicalization\nprocess, which strips out the username/password.\n", 
                    "is_in_email_summary": true
                }
            ], 
            "name": "Dave Camp (:campd)", 
            "email": "Dave Camp (:campd)"
        }, 
