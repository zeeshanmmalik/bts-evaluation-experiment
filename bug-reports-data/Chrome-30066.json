                {
                    "is_in_lex_summary": false, 
                    "text": "I think there are actually 2 related issues here:\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "1. Can't drag a submenu/subfolder -- won't go into drag mode.\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "2. Can't drag a bookmark to a subfolder:\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "   * Have a bookmark and a subfolder at the same level in a menu hierarchy.\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "   * Try to drag the bookmark into the subfolder.\n", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "   * When you do this, the menu disappears before you can drop the bookmark.  This\ncan be observed in any folder with a subfolder, as well as in the \"Other Bookmarks\" menu.\n", 
                    "is_in_email_summary": true
                }
