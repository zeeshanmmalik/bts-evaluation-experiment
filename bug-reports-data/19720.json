                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "java to 'return 60;'.", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "This will expire the cookie \nafter 60 seconds.", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
