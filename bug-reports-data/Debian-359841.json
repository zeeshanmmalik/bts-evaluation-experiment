                    "is_in_lex_summary": false, 
                    "text": "", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "- the bug was fixed in one version but subsequently found to exist\n    in a later version", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "", 
                    "is_in_email_summary": true
                }, 
                {
