            "sentences": [
                {
                    "is_in_lex_summary": true, 
                    "text": "Re: middle click emulation not working", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "Plug your mouse, run 'xinput list >xinput-list.txt' and attach the file here.", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "Middlemouse emulation defaults off now, but we might add a quirk for your mouse to use it by default.", 
                    "is_in_email_summary": true
                }
            ], 
            "name": "tjaalton", 
            "email": "tjaalton"
        }, 
