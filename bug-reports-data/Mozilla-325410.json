            "sentences": [
                {
                    "is_in_lex_summary": true, 
                    "text": "(From update of attachment 210322 [details])\n", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "(The NO_PKG_FILES ifdefs are because I override that variable, setting it to\nempty, during Camino packaging.)\n", 
