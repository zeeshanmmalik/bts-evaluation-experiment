                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "-\twget -r -nd --no-parent --level=1 -P \"$subarch\" -R 'index*' \"$DI_WWW_HOME/$subarch/cdrom/\"\n+\twget -r -nd --no-parent --level=1 -P \"$subarch\" -R 'index*' \"$DI_WWW_HOME/$subarch/cdrom${bitness}/\"\n   fi\n", 
