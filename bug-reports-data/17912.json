            "date": 1249038777, 
            "sentences": [
                {
                    "is_in_lex_summary": true, 
                    "text": "After talking with Ben, Glen, Cole, and pkasting, we're going to do what this bug \nrequests.", 
                    "is_in_email_summary": true
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "control-enter adds the www/.", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "com like on windows", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "command-enter opens the url in a new tab like on safari.", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": false, 
                    "text": "For the curious, control-enter was an old windows Netscape thing that people seem \nto have forgotten about (or never needed to implement) on the mac side.", 
                    "is_in_email_summary": false
                }, 
                {
                    "is_in_lex_summary": true, 
                    "text": "Since it's \nnot something everyone will know about, we'll favor the more current (and \nconsistent) behavior for command which is to open in a new tab.", 
                    "is_in_email_summary": false
                }
